
@{
    ViewBag.Title = "登入";
    Layout = "~/Views/Shared/_LoginLayout.cshtml";
}


<div class="login-brand">
    <img class="nav_logo4" src="~/Content/images/差勤打卡LOGO2.png"><span class="nav_logo-name2">&nbsp;Punch.</span>
</div>

<div class="card2 card-primary">
    <div class="card-header"><p>Sign in to Punch</p></div>

    <div class="card-body">
        <form method="POST" action="#" class="needs-validation" novalidate="">
            <div class="form-group">
                <label for="email">公司ID</label>
                <input id="email" type="email" class="form-control" name="email" placeholder="請輸入公司ID" tabindex="1" required autofocus>
            </div>

            <div class="form-group">
                <button type="submit" class="btn btn-primary btn-block" tabindex="4">
                    Login
                </button>
            </div>
        </form>
    </div>
</div>


